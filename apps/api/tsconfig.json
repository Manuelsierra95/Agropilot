{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "lib": ["ESNext"],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@env": ["env.d.ts"],
      "@workspace/types/*": ["../../packages/types/src/*"]
    },
    "types": ["@cloudflare/workers-types"]
  }
}
